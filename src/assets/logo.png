<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Circle -->
  <circle cx="100" cy="100" r="90" fill="#0066CC" stroke="#ffffff" stroke-width="4"/>
  
  <!-- Gradient Definition -->
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0066CC"/>
      <stop offset="100%" style="stop-color:#004499"/>
    </linearGradient>
  </defs>
  
  <!-- Main Circle -->
  <circle cx="100" cy="100" r="85" fill="url(#logoGradient)"/>
  
  <!-- House Icon -->
  <g transform="translate(100, 100)">
    <!-- Roof -->
    <path d="M -30 -10 L 0 -30 L 30 -10 Z" fill="#ffffff" opacity="0.9"/>
    <!-- House Body -->
    <rect x="-25" y="-10" width="50" height="35" fill="#ffffff" opacity="0.9"/>
    <!-- Door -->
    <rect x="-8" y="5" width="16" height="20" fill="#0066CC"/>
    <!-- Windows -->
    <rect x="-20" y="0" width="10" height="10" fill="#0066CC"/>
    <rect x="10" y="0" width="10" height="10" fill="#0066CC"/>
  </g>
  
  <!-- Text SR -->
  <text x="100" y="140" font-family="Arial, sans-serif" font-size="28" font-weight="bold" fill="#ffffff" text-anchor="middle">SR</text>
  
  <!-- Smart Rent Text -->
  <text x="100" y="160" font-family="Arial, sans-serif" font-size="10" fill="#ffffff" text-anchor="middle" opacity="0.8">SMART RENT</text>
</svg>